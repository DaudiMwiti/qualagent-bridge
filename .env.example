
# PostgreSQL
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=qualagents
POSTGRES_PORT=5432

# Database connection pool
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=1800

# Vector database settings
VECTOR_EMBEDDING_DIM=1536
VECTOR_INDEX_TYPE=ivfflat
VECTOR_INDEX_LISTS=100

# Search settings
SEARCH_RESULT_LIMIT=20
SEARCH_MIN_SCORE=0.0
SEARCH_CACHE_TTL=300

# Caching settings
ENABLE_CACHE=1
CACHE_CLEANUP_INTERVAL=3600

# Parameter extraction settings
PARAM_EXTRACTION_MAX_RETRIES=2

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=QualAgents

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here
DEFAULT_MODEL=gpt-4o

# Hybrid AI Architecture
HF_API_KEY=your-huggingface-api-key-here
REPLICATE_API_TOKEN=your-replicate-token-here
COHERE_API_KEY=your-cohere-api-key-here
SYMBL_API_KEY=your-symbl-api-key-here

# Feature Flags
USE_OPENSOURCE_EMBED=1
SENTIMENT_PROVIDER=hybrid
PARAM_EXTRACTION_PROVIDER=openai

# Model Settings
HF_EMBEDDING_MODEL=BAAI/bge-small-en
HF_SENTIMENT_MODEL=distilbert-base-uncased-emotion
REPLICATE_PARAM_MODEL=mistralai/mixtral-8x7b-instruct-v0.1
